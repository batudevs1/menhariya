<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:background="#079FAB">

    <TextView
            android:text="Menhariya"
            android:layout_width="137dp"
            android:layout_height="43dp"
            android:textSize="25dp"
            android:id="@+id/textView10" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.267" tools:layout_editor_absoluteY="39dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Name"
            android:background="#ffff"
            android:ems="10"
            android:id="@+id/name_Text"
            android:layout_marginTop="30dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="69dp"
            android:layout_marginStart="69dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="129dp"
            android:layout_marginRight="129dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="26dp"
            android:inputType="textPersonName"
            android:hint="User ID"
            android:background="#ffff"
            android:ems="10"
            android:id="@+id/userid_Text"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="129dp" android:layout_marginRight="129dp" android:layout_marginTop="47dp"
            app:layout_constraintTop_toBottomOf="@+id/name_Text" android:layout_marginBottom="42dp"
            app:layout_constraintBottom_toTopOf="@+id/bank_Text" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="69dp" android:layout_marginStart="69dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#ffff"
            android:inputType="textPersonName"
            android:hint="Bank Acc"
            android:ems="10"
            android:id="@+id/bank_Text"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="129dp" android:layout_marginRight="129dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="69dp"
            android:layout_marginStart="69dp" android:layout_marginTop="37dp"
            app:layout_constraintTop_toBottomOf="@+id/userid_Text"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:inputType="textPassword"
            android:hint="Password"
            android:background="#ffff"
            android:ems="10"
            android:id="@+id/password_Text"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="129dp" android:layout_marginRight="129dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="69dp"
            android:layout_marginStart="69dp" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/bank_Text"/>
    <Button
            android:text="SignUp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/signup_Button"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="118dp" android:layout_marginStart="118dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="205dp"
            android:layout_marginRight="205dp" android:layout_marginBottom="312dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
    />

</androidx.constraintlayout.widget.ConstraintLayout>
